<div class="sidebar">
    <div class="sidebar-search">
        <input type="text" class="form-control" id="search" placeholder="Search..." autocomplete="off">
        <div class="list-group sidebar-results" id="results"></div>
    </div>
    {% for sidebar in nav %}
        {% for title, sidebar in sidebar %}
            <div class="sidebar-subtitle">{{ title }}</div>
            {% for item in sidebar %}
                <a class="sidebar-link{% if docs.title == item.title %} active{% endif %}" href="{% if item.url %}{{ item.url }}{% else %}{{ item.title|lower|replace({' ': "-"}) }}.html{% endif %}"><span>{{ item.title }}</span></a>
            {% endfor %}
        {% endfor %}
    {% endfor %}
</div>
