<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <!-- meta -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {% if meta.description %}
        <meta name="description" content="{{ meta.description }}">
    {% endif %}

    <title>Documentation v{{ config.version }} | {{ config.title }}</title>

    <!-- css: plugins -->
    {% if block("css") is defined %}
        {% block css %}{% endblock %}
    {% endif %}
    <link rel="stylesheet" href="assets/plugins/highlight/styles/github.css">

    <!-- css: theme -->
    <link rel="stylesheet" href="../dist/css/theme.css">
    <link rel="stylesheet" href="assets/css/docs.min.css">
</head>
<body class="fixed-navbar">
    <div class="site">
        <header class="site-header">
            {% include 'header.twig' %}
        </header>
        <!-- end .site-header -->

        {% include 'sidebar.twig' %}
        <!-- end .sidebar -->

        <div class="site-content" role="main">
            <section>
                <div class="container">
                    <div class="row">
                        <div class="col{% if docs.blocks %}-9{% endif %}">
                            {% include 'content.twig' %}
                        </div>
                        {% if docs.blocks %}
                            <div class="col-3">
                                <nav class="nav nav-docs flex-column" ya-sticky="top: 100">
                                    {% for nav in docs.blocks %}
                                        <a class="nav-link" href="#{{ nav.title|lower|replace({' ': "-", ':': "", '.': ""}) }}">{{ nav.title }}</a>
                                    {% endfor %}
                                    {% if docs.bootstrap %}
                                        <a class="nav-link" href="https://getbootstrap.com/docs/{{ config.bootstrap }}/{{ docs.bootstrap }}/" target="_blank">Bootstrap</a>
                                    {% endif %}
                                    <div class="divider"></div>
                                    <a class="nav-link" href="https://github.com/yakuthemes/gameforest/issues" target="_blank"><i class="ya ya-help"></i> Report issue</a>
                                    <a class="nav-link" href="https://themeforest.net/item/gameforest-responsive-gaming-html-theme/5007730/comments" target="_blank"><i class="ya ya-link"></i> Get help</a>
                                </nav>
                            </div>
                        {% endif %}
                    </div>
                </div>
            </section>
        </div>
        <!-- end .site-content -->

        <a class="version" href="changelog.html" data-toggle="tooltip" data-placement="left" title="Gameforest version"><i class="ya ya-bulb mr-1"></i> v{{ config.version }}</a>

        <footer class="docs-footer">
           {% include 'footer.twig' %}
        </footer>
    </div>
    <!-- end .site -->

    <!-- js: vendor -->
    <script src="../dist/js/vendor.js"></script>
    <script src="../dist/js/theme.min.js"></script>
    <script src="assets/plugins/holder/holder.min.js"></script>
    <script src="assets/plugins/clipboard/clipboard.min.js"></script>
    <script src="assets/plugins/highlight/highlight.pack.js"></script>
    <script src="assets/js/docs.min.js"></script>
    {% if block("js") is defined %}
        {% block js %}{% endblock %}
    {% endif %}
    <script>
        var gameforest = {
            disqus: '{{ config.disqus }}',
            facebook: {
                lang: '{{ config.facebook.lang }}',
                version: '{{ config.facebook.version }}',
                id: '{{ config.facebook.appID }}',
            }
        }
    </script>
</body>
</html>
